  "paths": {
    "/api/Actors": {
      "get": {
        "tags": [ "Actors" ],
        "summary": "Search for Actors",
        "description": "Returns a JSON array of Actor documents",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "The term used to search Actors by name",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "maxLength": 100,
              "minLength": 3,
              "type": "string",
              "example": "keanu"
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "description": "1 based page index",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "maximum": 10000,
              "minimum": 1,
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "page size",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "maximum": 1000,
              "minimum": 1,
              "type": "integer",
              "default": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "JSON array of Actor documents Empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Actor"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameter",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Invalid search parameter"
                }
              }
            }
          }
        }
      }
    },
