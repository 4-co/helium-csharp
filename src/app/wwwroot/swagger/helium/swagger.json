  "paths": {
    "/api/Actors": {
      "get": {
        "tags": [
          "Actors"
        ],
        "summary": "",
        "description": "Returns a JSON array of Actor documents",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "(optional) The term used to search Actors by name (valid: >= 3 characters)",
            "schema": {
              "type": "string",
              "default": null
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "description": "(optional) 1 based page index (valid: 1 - 10,000)",
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "(optional) page size (valid: 1 - 1000)",
            "schema": {
              "type": "integer",
              "default": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "JSON array of Actor documents or empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Actor"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameter"
          }
        }
      }
    },
    "/api/Actors/{actorId}": {
      "get": {
        "tags": [
          "Actors"
        ],
        "summary": "",
        "description": "Returns a single JSON Actor document by actorId",
        "parameters": [
          {
            "name": "actorId",
            "in": "path",
            "description": "The actorId",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Actor"
                }
              }
            }
          },
          "400": {
            "description": "Invalid actorId"
          },
          "404": {
            "description": "actorId not found"
          }
        }
      }
    },
