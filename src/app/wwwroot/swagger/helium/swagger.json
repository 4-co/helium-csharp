    "/api/Movies": {
      "get": {
        "tags": [ "Movies" ],
        "summary": "Search for Movies",
        "description": "Returns a JSON array of Movie documents",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "The term used to search by Movie title <br> ```valid: 3 - 50 characters```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "matrix"
            }
          },
          {
            "name": "genre",
            "in": "query",
            "description": "Returns a JSON array of Movie documents filtered by genre<br> ```valid: 3 - 50 characters```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "action"
            }
          },
          {
            "name": "year",
            "in": "query",
            "description": "Get movies by year <br>\n  ```valid: 1873 - currentYear + 5```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 2019
            }
          },
          {
            "name": "rating",
            "in": "query",
            "description": "Get movies with a rating >= rating <br>\n  ```valid: 0.0 - 10.0```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "number",
              "example": 8.8
            }
          },
          {
            "name": "topRated",
            "in": "query",
            "description": "Get top rated movies <br> ```valid: true or false```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "boolean",
              "example": true,
              "default": false
            }
          },
          {
            "name": "actorId",
            "in": "query",
            "description": "Get movies by Actor Id <br>\n  ```valid: nm0000206```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "nm0000206"
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "description": "1 based page index <br>\n  ```valid: 1 - 10,000```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 10,
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "page size <br>\n  ```valid 1 - 1000```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 2,
              "default": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "JSON array of Movie documents or empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Movie"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameter",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Invalid search parameter"
                }
              }
            }
          }
        }
      }
    },
