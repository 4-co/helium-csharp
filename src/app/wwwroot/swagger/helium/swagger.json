    "/api/Movies": {
      "get": {
        "tags": [
          "Movies"
        ],
        "summary": "",
        "description": "Returns a JSON array of Movie documents",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "(optional) The term used to search by Movie title (valid: 3 - 100 characters)",
            "schema": {
              "type": "string",
              "default": null
            }
          },
          {
            "name": "genre",
            "in": "query",
            "description": "(optional) Returns a JSON array of Movie documents filtered by genre (valid: 3- 50 characters)",
            "schema": {
              "type": "string",
              "default": null
            }
          },
          {
            "name": "year",
            "in": "query",
            "description": "(optional) Get movies by year (valid: 1873 - currentYear + 5)",
            "schema": {
              "type": "integer",
              "default": null
            }
          },
          {
            "name": "rating",
            "in": "query",
            "description": "(optional) Get movies with a rating >= rating (8.5)",
            "schema": {
              "type": "number",
              "default": null
            }
          },
          {
            "name": "topRated",
            "in": "query",
            "description": "(optional) Get top rated movies (valid: true | false)",
            "schema": {
              "type": "boolean",
              "default": null
            }
          },
          {
            "name": "actorId",
            "in": "query",
            "description": "(optional) Get movies by Actor Id (valid: valid actorId)",
            "schema": {
              "type": "string",
              "default": null
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "description": "1 based page index (valid: 1 - 10,000)",
            "schema": {
              "type": "integer",
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "page size (valid 1 - 1000)",
            "schema": {
              "type": "integer",
              "default": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "JSON array of Movie documents or empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Movie"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid parameter"
          }
        }
      }
    },
